<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
  <PropertyGroup>
    <PackageProjectUrl>https://github.com/penguincms/Penguin.Persistence.Abstractions</PackageProjectUrl>
    <Version>3.0.1</Version>
    <Description>Abstract/Light classes created to help decouple applications from their persistence systems, mainly created to remove Penguins from EntityFramework and allow for in-memory or JSON persistence through a single code path</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Penguin.Persistence.Abstractions</AssemblyName>
    <RootNamespace>Penguin.Persistence.Abstractions</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Penguin.Entities.Abstractions" Version="2.0.1-T9M2XAX" />
    <PackageReference Include="Penguin.Reflection.Extensions" Version="3.0.5-T9M2XAX" />
  </ItemGroup>
</Project>